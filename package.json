{
    "name": "phenomenon-react-ts-template",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "engines": {
        "node": ">=16.0.0",
        "yarn": "please-use-npm",
        "npm": ">=7.10.0"
    },
    "scripts": {
        "dev": "vite --open --host",
        "build": "tsc && vite build",
        "preview": "vite preview --open --host",
        "lint:eslint": "eslint --cache --max-warnings 0 --report-unused-disable-directives \"src/**/*.{js,jsx,ts,tsx}\"",
        "lint:stylelint": "stylelint --cache -q \"**/*.css\"",
        "lint:prettier": "prettier --check \"**/*.{js,ts,tsx,jsx,json,html,css,cjs}\"",
        "lint:prettier:fix": "prettier --write \"**/*.{js,ts,tsx,jsx,json,html,css,cjs}\"",
        "lint": "npm run lint:eslint && npm run lint:stylelint && npm run lint:prettier",
        "prepare": "husky install"
    },
    "dependencies": {
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.3.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.6.7",
        "@commitlint/config-conventional": "^17.6.7",
        "@types/react": "^18.2.17",
        "@types/react-dom": "^18.2.7",
        "@typescript-eslint/eslint-plugin": "^6.2.0",
        "@typescript-eslint/parser": "^6.2.0",
        "@vitejs/plugin-react": "^4.0.3",
        "autoprefixer": "^10.4.14",
        "eslint": "^8.45.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^5.0.0",
        "eslint-plugin-react": "^7.33.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "husky": "^8.0.3",
        "postcss": "^8.4.27",
        "prettier": "^3.0.0",
        "rollup-plugin-visualizer": "^5.9.2",
        "stylelint": "^15.10.2",
        "stylelint-config-standard": "^34.0.0",
        "tailwindcss": "^3.3.3",
        "typescript": "^5.1.6",
        "vite": "^4.4.7",
        "vite-plugin-eslint": "^1.8.1"
    }
}
